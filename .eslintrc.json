{
    "env": {
        "browser": true,
        "es2020": true,
        "jquery": true
    },
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "plugins": [
        "class-property",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ],    
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "no-lonely-if": "off",
        "no-underscore-dangle": "off"
    }
}
